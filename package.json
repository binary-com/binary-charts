{
  "name": "binary-charts",
  "version": "7.18.2",
  "description": "Binary trade and contract visualisation library, written in React",
  "contributors": [
    {
      "name": "Boris Yankov",
      "email": "boris@binary.com"
    },
    {
      "name": "Lim Qing Wei",
      "email": "qingwei@binary.com"
    }
  ],
  "main": "lib/binary-charts.js",
  "scripts": {
    "start": "start-storybook -p 9001",
    "test": "npm run test:eslint && mocha --compilers js:babel-core/register src/__tests__/*.js src/*/__tests__/*.js -R progress",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "storybook": "build-storybook -o dist",
    "ghpages": "git subtree push --prefix dist origin gh-pages",
    "prepublish": "npm run build",
    "ci": "webpack --config webpack.config.js --progress --colors --watch",
    "test:coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha -- --opts mocha.opts",
    "test:coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf ./coverage",
    "test:eslint": "webpack --config webpack.config.js",
    "test:flow": "flow check --all --show-all-errors",
    "test:full": "npm run test:eslint && npm run test:coverage"
  },
  "keywords": [
    "binary.com"
  ],
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "binary-utils": "*",
    "fbjs": "^0.8.4",
    "highcharts": "^4.2.6"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.3.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "binary-live-api": "^20.5.4",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-flowtype": "^2.11.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "webpack": "^2.1.0-beta.4"
  },
  "peerDependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  }
}
